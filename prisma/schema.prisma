generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Events {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  description String
  end_date    String
  start_date  String
  title       String
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  name     String
  password String
}
